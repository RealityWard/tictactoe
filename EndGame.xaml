<Window x:Class="tictactoe.EndGame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tictactoe"
        mc:Ignorable="d"
        Title="EndGame" Height="140" Width="320">
  <Grid Background="#FF7E4BA4">
    <Grid.RowDefinitions>
      <RowDefinition Height="2*"/>
      <RowDefinition/>
    </Grid.RowDefinitions>
    <Grid.ColumnDefinitions>
      <ColumnDefinition/>
      <ColumnDefinition/>
    </Grid.ColumnDefinitions>
    <Label x:Name="labelText" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.ColumnSpan="2"/>
    <Button x:Name="button" Content="Restart" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Center" IsDefault="True" TabIndex="1" Padding="2,1,2,1" Grid.IsSharedSizeScope="True" Click="RestartBtnOnClick" Background="{x:Null}"/>
    <Button x:Name="button1" Grid.Column="1" Content="Return to Main Menu" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Center" TabIndex="2" Padding="2,1,2,1" Click="ReturnToMainMenuOnClick" Background="{x:Null}"/>
  </Grid>
</Window>
